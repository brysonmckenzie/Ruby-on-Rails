<%#<h1>Dojos#index</h1>
<p>Find me in app/views/dojos/index.html.erb</p>%>

<h1>Listing Dojos</h1>
<table>
    <thead>
        <tr>
            <th>Coding Dojo Branch</th>
            <th>Address</th>
            <th>City</th>
            <th>State</th>
            <th>Actions</th>
        </tr>
    </thead>
    <% @dojos.each do |d| %>
    <tbody>
        <tr>
        <td> <a href="/dojos/<%=d.id%>/students"><%=d.branch%></a> </td>
            <td> <%=d.street%> </td>
            <td> <%=d.city%> </td>
            <td> <%=d.state%> </td>
            <td><a href="/dojos/<%=d.id%>">Show</a>
            </td>
        </tr>
    </tbody>
    <% end %>
</table><br>

